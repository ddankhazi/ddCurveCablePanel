// Curves 
global proc ddGenCurvesBetweenLocators()
{
    //Dialog popup to get a value from the user
    string $resultGravity = `promptDialog 
    -title "Enter Gravity" 
    -message "Gravity:" 
    -text "0.1"
    -button "OK" 
